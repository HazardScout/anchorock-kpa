  "main": "function/index.ts",
  "devDependencies": {
    "@types/aws-lambda": "^8.10.131",
    "@types/jest": "^29.5.11",
    "@types/node": "^20.10.8",
    "esbuild": "^0.19.11",
    "jest": "29.7.0",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "typescript": "^5.3.3"
  },
  "dependencies": {
    "aws-lambda": "^1.0.7",
    "aws-sdk": "2.1484.0",
    "aws-xray-sdk-core": "3.5.3",
    "axios": "1.4.0",
    "dotenv": "^16.3.1",
    "glob": "7.1.6",
    "jest": "29.6.2",
    "mime": "^1.6.0",
    "mongodb": "^6.0.0",
